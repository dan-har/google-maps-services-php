{
    "name": "dan-har/google-maps-services-php",
    "description": "Google maps web services php client.",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Har",
            "email": "dan@niddit.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "mockery/mockery": "^0.9.7"
    },
    "autoload": {
        "psr-4": {
            "Niddit\\Google\\Maps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niddit\\Google\\Maps\\Test\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
